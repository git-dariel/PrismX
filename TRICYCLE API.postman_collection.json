					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Store the token automatically after login response\r",
									"if (pm.response.code === 200) {\r",
									"    const responseJson = pm.response.json();\r",
									"    \r",
									"    if (responseJson.success && responseJson.data && responseJson.data.token) {\r",
									"        // Store token in environment variable named \"Token\"\r",
									"        pm.environment.set(\"token\", responseJson.data.token);\r",
									"        \r",
									"        console.log(\"âœ… Token automatically stored in environment!\");\r",
									"        console.log(\"Token preview: \" + responseJson.data.token.substring(0, 20) + \"...\");\r",
									"    }\r",
